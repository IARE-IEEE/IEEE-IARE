<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@5.15.4/css/fontawesome.min.css"
      integrity="sha384-jLKHWM3JRmfMU0A5x5AkjWkw/EYfGUAGagvnfryNV3F9VqM98XiIH7VBGVoxVSc7"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/css/style.css" />
    <link
      rel="shortcut icon"
      href="/images/ieee-potrait logo.svg"
      type="image/x-icon"
    />
    <title>IEEE IARE Events</title>
    <script
      type="module"
      src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"
    ></script>
  </head>
  <body>
    <div class="evconatiner">
      <div class="navbar">
        <a href="ieee.html">
          <img src="images/ieee-potrait logo.svg" class="EVlogo" />
        </a>
        <nav>
          <ul>
            <li><a href="/">Home</a></li>
            <li><a href="/members">Members</a></li>
            <li><a href="">CS</a></li>
            <li><a href="">WIE</a></li>
            <li><a href="">SP</a></li>
            <li><a href="">RA</a></li>
            <!--<li><a href="CS_E.html">CS E</a></li>
            <li><a href="CS_M.html">CS M</a></li>
            <li><a href="WIE_E.html">WIE E</a></li>
            <li><a href="WIE_M.html">WIE M</a></li>-->
          </ul>
        </nav>
      </div>
    </div>
    <div class="Ecards">
      <div class="Members2">
        <div class="membersheading" style="margin-bottom: 20px;">Events</div>
      </div>
      <% for(var i=0; i<qrs.length; i++) {%>
      <div class="ECard-container">
        <div class="number"><%= qrs[i].Branch %></div>
        <div class="EUpper-container">
          <div class="EImg-container">
            <img src="/images/ieee-potrait logo.svg" alt="" class="eventclass"/>
          </div>
        </div>
        <div class="ELower-container">
          <div class="event-details">
            <p class="evtitle"><%= qrs[i].Name %></p>
            <p class="evdate"><%= qrs[i].Date %></p>
          </div>
          <div class="evbtn">
            <a href="<%= qrs[i].URL %>" class="btn">Details</a>
          </div>
        </div>
      </div>
      <% } %>
      <script>
        var CardUpper = document.querySelectorAll(".EUpper-container");
        var CardBtn = document.querySelectorAll(".btn");
        var number = document.querySelectorAll(".number");
        var eveclass=document.querySelectorAll(".eventclass")
        for (var i = 0; i < number.length; i++) {
          if (number[i].innerHTML == "SB") {
            CardUpper[i].style.background = "rgb(60, 48, 87)";
            CardBtn[i].style.background = "rgb(60, 48, 87)";
            eveclass[i].src="/images/ieee-potrait logo.svg"
          }
          if (number[i].innerHTML == "WIE") {
            CardUpper[i].style.background = "rgb(159, 30, 163)";
            CardBtn[i].style.background = "rgb(159, 30, 163)";
            eveclass[i].src="/images/Women Engineering logo.svg"
          }
          if (number[i].innerHTML == "CS") {
            CardUpper[i].style.background = "rgb(241, 157, 31)";
            CardBtn[i].style.background = "rgb(241, 157, 31)";
            eveclass[i].src="/images/CS IEEE Potrait.svg"
          }
          if (number[i].innerHTML == "SP") {
            CardUpper[i].style.background = "rgb(120,190,32)";
            CardBtn[i].style.background = "rgb(120,190,32)";
            eveclass[i].src="/images/SP Logo.svg"
          }
          if (number[i].innerHTML == "RA") {
            CardUpper[i].style.background = "rgb(152,1,46)";
            CardBtn[i].style.background = "rgb(152,1,46)";
            eveclass[i].src="/images/RA Logo.svg"
          }
        }
      </script>
    </div>
    <footer>
      <div class="footerclass">
        <div class="waves">
          <div class="wave" id="wave1"></div>
          <div class="wave" id="wave2"></div>
          <div class="wave" id="wave3"></div>
        </div>
        <ul class="social_media">
          <li>
            <a href="#"><ion-icon name="logo-instagram"></ion-icon></a>
          </li>
          <li>
            <a href="#"><ion-icon name="mail-open-outline"></ion-icon></a>
          </li>
        </ul>
        <ul class="menu">
          <li><a href="/">Home</a></li>
          <li><a href="/events">Events</a></li>
          <li><a href="/members">Members</a></li>
          <li><a href="">CS</a></li>
          <li><a href="">WIE</a></li>
          <li><a href="">SP</a></li>
          <li><a href="">RA</a></li>
          <!--<li><a href="CS_E.html">CS E</a></li>
        <li><a href="CS_M.html">CS M</a></li>
        <li><a href="WIE_E.html">WIE E</a></li>
        <li><a href="WIE_M.html">WIE M</a></li>-->
        </ul>
        <p>
            Copyright Â©
            <script>
              var CurrentYear = new Date().getFullYear();
              document.write(CurrentYear);
            </script>
          </p>
      </div>
    </footer>
  </body>
</html>
